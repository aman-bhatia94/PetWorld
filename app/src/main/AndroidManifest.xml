<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ateam.petworld">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activities.BookAppointment" />
        <activity android:name=".activities.Profile" />
        <activity android:name=".activities.SearchSitters">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ateam.petworld.activities.SitterDashboard" />
        <activity android:name="com.ateam.petworld.activities.OwnerDashboard" />
        <activity android:name="com.ateam.petworld.activities.RegisterActivity" />
        <activity android:name="com.ateam.petworld.activities.LoginActivity"></activity>
        <activity android:name="com.ateam.petworld.MainActivity" />
    </application>

</manifest>